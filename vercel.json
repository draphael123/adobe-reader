{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "pdf-screenshot-tool",
  "headers": [
    {
      "source": "/PDFScreenshotTool.zip",
      "headers": [
        { "key": "Content-Type", "value": "application/zip" },
        { "key": "Content-Disposition", "value": "attachment; filename=PDFScreenshotTool.zip" }
      ]
    },
    {
      "source": "/PDFScreenshotTool.exe",
      "headers": [
        { "key": "Content-Type", "value": "application/octet-stream" },
        { "key": "Content-Disposition", "value": "attachment; filename=PDFScreenshotTool.exe" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/download", "destination": "/PDFScreenshotTool.zip" }
  ]
}
